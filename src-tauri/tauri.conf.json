{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "ustb-wifi-tools",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "http": {
        "all":true,
        "request": true,
        "scope": [
          "http://202.204.60.7:8080/*"
        ]
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "ustb-wifi-tools",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "dangerousUseHttpScheme": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "ustb.wifi.tools",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
